  <style>
    .topnav {
      overflow: hidden;
      background-color: #333;
    }

    .topnav a {
      float: left;
      display: block;
      color: #f2f2f2;
      text-align: center;
      padding: 14px 16px;
      text-decoration: none;
      font-size: 17px;
    }

    .active {
      background-color: #4CAF50;
      color: white;
    }

    .topnav .icon {
      display: none;
    }

    .dropdown12 {
      float: left;
      overflow: hidden;
    }

    .dropdown12 .dropbtn {
      font-size: 17px;
      border: none;
      outline: none;
      color: white;
      padding: 14px 16px;
      background-color: inherit;
      font-family: inherit;
      margin: 0;
    }

    .dropdown12:hover {

      display: block !important;
      z-index: 150;
    }

    .dropdown12-content {
      display: none;
      position: absolute;
      background-color: #f9f9f9;
      min-width: 160px;
      box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
      z-index: 1;
    }

    .dropdown12-content a {
      float: none;
      color: black;
      padding: 8px 16px;
      text-decoration: none !important;
      display: block;
      text-align: left;
    }

    .topnav a:hover,
    .dropdown12:hover .dropbtn {
      background-color: #555;
      color: white;
    }

    .dropdown12-content a:hover {
      background-color: #ddd;
      color: black;
      z-index: 150;
    }

    .dropdown12:hover .dropdown12-content {
      display: block !important;
    }

    .topnav.responsive .dropdown12-content {
      position: relative;
    }

    @media screen and (max-width: 600px) {

      .topnav a:not(:first-child),
      .dropdown12 .dropbtn {
        display: none;
      }

      .topnav a.icon {
        float: right;
        display: block;
      }
    }

    @media screen and (max-width: 600px) {
      .topnav.responsive {
        position: relative;
      }

      .topnav.responsive .icon {
        position: absolute;
        right: 0;
        top: 0;
      }

      .topnav.responsive a {
        float: none;
        display: block;
        text-align: left;
      }

      .topnav.responsive .dropdown12 {
        float: none;
      }

      .topnav.responsive .dropdown12-content {
        position: relative;
      }

      .topnav.responsive .dropdown12 .dropbtn {
        display: block;
        width: 100%;
        text-align: left;
      }
    }

    .loadingText {
      font-size: 20px !important;
    }

    body {
      margin: 0;
      padding: 0;
    }
  </style>
